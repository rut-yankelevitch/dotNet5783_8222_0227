<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:loc="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <loc:Categorys x:Key="Categorys"/>
        <loc:EnumToStringConverter x:Key ="EnumToStringConverter"/>
        <loc:ConvertZeroToBool x:Key="ConvertZeroToBool"/>
        <loc:ConvertZeroToVisible x:Key="ConvertZeroToVisible"/>
        <loc:ConvertZeroToHidden x:Key="ConvertZeroToHidden"/>
        <loc:ConvertInputToTrue x:Key="ConvertInputToTrue"/>
        <loc:ConverIntToString x:Key="ConverIntToString"/>
        <loc:ConverDoubleToString x:Key="ConverDoubleToString"/>
        <loc:DateTimeConverter x:Key="DateTimeConverter"/>
        <loc:NextStatusConverter x:Key="NextStatusConverter"/>
        <loc:StatusToHiddenConverter x:Key="StatusToHiddenConverter"/>
        <loc:ConvertStatusToVisible x:Key="ConvertStatusToVisible"/>
    </Application.Resources>
</Application>